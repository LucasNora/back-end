programa {


  inclua biblioteca Util --> u
  inteiro nomeMatriz[60][60]

  funcao inicio() {

    para(inteiro i = 0; i < 60; i++){
        para(inteiro j = 0; j < 60; j++){
          escreva(" ", u.sorteia(0,1) , " ")
                           
      }
       escreva (u.aguarde(1000), "\n")
    }
  }
}

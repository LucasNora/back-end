programa {


  inclua biblioteca Util --> u
  inteiro nomeMatriz[6][6]

  funcao inicio() {

    para(inteiro i = 0; i < 6; i++){
        para(inteiro j = 0; j < 6; j++){
          nomeMatriz[i][j] = u.sorteia(0,1)
          escreva(" ", u.sorteia(0,3) , " ")
          
                           
      }
       escreva (u.aguarde(100), "\n")
    }
  }
}
